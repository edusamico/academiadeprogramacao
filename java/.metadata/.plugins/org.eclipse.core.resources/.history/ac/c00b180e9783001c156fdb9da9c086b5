package main;

public class Main {
	
	public static void main (String[] args) {
		Book bookJava = new Book("Java como programar", 100.0f, "Bookman");
		Book bookC = new Book("Java como programar", 100.0f, "Bookman");
		Item pencil = new Item("Lapis", 100.0f);
		
		Item[] myStore = new Item [3];
		myStore[0] = bookJava;
		myStore[1] = bookC;
		myStore[2] = pencil;
		
		for (int i = 0; i < 3; i++) {
			System.out.println(myStore[i].toString());
		}
	
	}
	
}